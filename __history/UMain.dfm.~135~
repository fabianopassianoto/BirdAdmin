  object QryTop: TZQuery
    Connection = Conexao
    SQL.Strings = (
      'select max(t.mon$top_transaction) as maxtop,'
      '       min(t.mon$top_transaction) as mintop,'
      '       count(s.MON$ATTACHMENT_ID) as Usuarios'
      '      from MON$TRANSACTIONS t'
      
        '      join MON$ATTACHMENTS s on s.MON$ATTACHMENT_ID = t.MON$ATTA' +
        'CHMENT_ID'
      '      where s.MON$STATE = 1')
    Params = <>
    Left = 648
    Top = 216
    object QryTopMAXTOP: TLargeintField
      FieldName = 'MAXTOP'
      ReadOnly = True
    end
    object QryTopMINTOP: TLargeintField
      FieldName = 'MINTOP'
      ReadOnly = True
    end
    object QryTopUSUARIOS: TLargeintField
      FieldName = 'USUARIOS'
      ReadOnly = True
    end
  end
  object DsTop: TDataSource
    DataSet = QryTop
    Left = 616
    Top = 216
  end
end
