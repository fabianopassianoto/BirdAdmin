  object QryConexoes: TZQuery
    Connection = Conexao
    SQL.Strings = (
      'select'
      
        '    datediff(day from mon$timestamp to current_timestamp) || '#39' d' +
        'ias '#39' ||'
      
        '    cast( trunc(trunc(cast(datediff(minute from mon$timestamp to' +
        ' current_timestamp) as numeric(9,2)) / 60) -'
      
        '        (datediff(day from mon$timestamp to current_timestamp) *' +
        ' 24)) as numeric(2,0)) || '#39' horas '#39' ||'
      
        '    mod(cast(datediff(minute from mon$timestamp to current_times' +
        'tamp) as numeric(9,2)), 60) ||'#39' m.'#39' as Ativo,'
      '       mon$attachment_id as Conexao,'
      '       mon$server_pid as Pid,'
      '       case when mon$state = 0 then'
      '          '#39'Ocioso'#39
      '       else case when mon$state = 1 then'
      '          '#39'Ativo'#39
      '       else'
      '          '#39'Parado'#39
      '       end end   as Status,'
      '       mon$attachment_name as Banco,'
      '       mon$user as Usuario,'
      '       mon$remote_address as Host,'
      '       mon$remote_host as Remote,'
      '       mon$remote_os_user as UserPC,'
      '       mon$remote_process,'
      '      mon$client_version'
      
        '       -- datediff(day from mon$timestamp to current_timestamp) ' +
        'DIAS,'
      
        '       --  trunc(trunc(cast(datediff(minute from mon$timestamp t' +
        'o current_timestamp) as numeric(9,2)) / 60) -'
      
        '       -- (datediff(day from mon$timestamp to current_timestamp)' +
        ' * 24)) HORAS,'
      
        '       -- mod(cast(datediff(minute from mon$timestamp to current' +
        '_timestamp) as numeric(9,2)), 60) MINUTOS,'
      ''
      ''
      ''
      '       from mon$attachments'
      ''
      '')
    Params = <>
    Left = 648
    Top = 136
