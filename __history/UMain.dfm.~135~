  object QryTransaction: TZQuery
    Connection = Conexao
    SQL.Strings = (
      'select s.mon$attachment_id as Conexao,'
      ''
      
        '    datediff(day from c.mon$timestamp to current_timestamp) || '#39 +
        ' dias '#39' ||'
      
        '    cast( trunc(trunc(cast(datediff(minute from c.mon$timestamp ' +
        'to current_timestamp) as numeric(9,2)) / 60) -'
      
        '        (datediff(day from c.mon$timestamp to current_timestamp)' +
        ' * 24)) as numeric(2,0)) || '#39' horas '#39' ||'
      
        '    mod(cast(datediff(minute from c.mon$timestamp to current_tim' +
        'estamp) as numeric(9,2)), 60) ||'#39' m.'#39' as TempoConexao,'
      '    t.mon$transaction_id as Transacao,'
      
        '    cast( trunc(trunc(cast(datediff(minute from t.mon$timeStamp ' +
        'to current_timestamp) as numeric(9,2)) / 60) -'
      
        '        (datediff(day from t.mon$timeStamp to current_timestamp)' +
        ' * 24)) as numeric(2,0)) || '#39' horas '#39' ||'
      
        '    mod(cast(datediff(minute from t.mon$timeStamp to current_tim' +
        'estamp) as numeric(9,2)), 60) ||'#39' m.'#39' as TempoTransacao,'
      '    case when t.mon$state = 0 then'
      '          '#39'Ocioso'#39
      '       else case when t.mon$state = 1 then'
      '          '#39'Ativo'#39
      '       else'
      '          '#39'Parado'#39
      '       end end   as StatusTransacao,'
      '      s.mon$statement_id,'
      ''
      ''
      ''
      
        '      c.mon$remote_address as Host, c.mon$remote_process, s.MON$' +
        'SQL_TEXT, i.*'
      '      from MON$STATEMENTS s'
      '      join MON$RECORD_STATS i on i.MON$STAT_ID = s.MON$STAT_ID'
      
        '      join MON$TRANSACTIONS t on t.mon$attachment_id = s.mon$att' +
        'achment_id'
      
        '      join mon$attachments c on c.mon$attachment_id = s.mon$atta' +
        'chment_id'
      ''
      ''
      ''
      '--select s.*  from MON$STATEMENTS s')
    Params = <>
    Left = 648
    Top = 168
    object QryTransactionCONEXAO: TLargeintField
